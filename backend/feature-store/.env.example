# Feature Store Service Configuration

# HTTP Server
HTTP_HOST=0.0.0.0
HTTP_PORT=8010

# gRPC Server
GRPC_HOST=0.0.0.0
GRPC_PORT=9010

# PostgreSQL (for metadata)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/nova

# Redis (hot features)
REDIS_URL=redis://localhost:6379
REDIS_DEFAULT_TTL_SECONDS=3600

# ClickHouse (near-line features)
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_DATABASE=feature_store
CLICKHOUSE_SYNC_INTERVAL_SECONDS=300

# Feature Serving Config
FEATURE_CACHE_SIZE=10000
BATCH_FETCH_MAX_SIZE=100

# Observability
LOG_LEVEL=info
ENABLE_METRICS=true
RUST_LOG=feature_store=debug,info
