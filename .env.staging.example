# Nova Staging 环境变量示例
# 复制到 .env.staging 并填入实际值

# ============= AWS 配置 =============
AWS_REGION=ap-northeast-1
AWS_ACCOUNT_ID=YOUR_ACCOUNT_ID_HERE

# ============= Terraform =============
TF_VAR_aws_region=ap-northeast-1
TF_VAR_environment=staging

# ============= Kubernetes =============
KUBE_NAMESPACE=nova-staging
KUBE_CLUSTER_NAME=nova-staging-eks

# ============= 数据库 =============
# PostgreSQL - 从 AWS Secrets Manager 获取
DB_HOST=postgres.nova-staging.svc.cluster.local
DB_PORT=5432
DB_USER=nova_staging
DB_PASSWORD=YOUR_DB_PASSWORD_HERE
DB_NAME=nova_auth

# ============= 应用配置 =============
ENV=staging
LOG_LEVEL=debug

# ============= 第三方服务 =============
# ClickHouse
CLICKHOUSE_HOST=clickhouse.nova-staging.svc.cluster.local
CLICKHOUSE_PORT=9000
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=YOUR_CLICKHOUSE_PASSWORD_HERE

# Redis
REDIS_HOST=redis.nova-staging.svc.cluster.local
REDIS_PORT=6379

# S3
S3_BUCKET=nova-staging-media
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY_HERE
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_KEY_HERE

# ============= 安全配置 =============
JWT_SECRET=YOUR_JWT_SECRET_HERE
JWT_REFRESH_SECRET=YOUR_JWT_REFRESH_SECRET_HERE
